        "node_compatibility": true
      }
    },
    "Order_backend": {
      "type": "motoko",
      "main": "backend/Order_backend/Order.mo",
      "declarations": {
        "node_compatibility": true
      }
    },
    "Client_backend": {
      "type": "motoko",
      "main": "backend/Client_backend/Client.mo",
      "declarations": {
        "node_compatibility": true
      }
    },
    "Freelancer_backend": {
      "type": "motoko",
      "main": "backend/Freelancer_backend/Freelancer.mo",
      "declarations": {
        "node_compatibility": true
      }
    },
    "Review_backend": {
      "type": "motoko",
      "main": "backend/Review_backend/Review.mo",
      "declarations": {
